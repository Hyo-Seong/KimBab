        <TextBlock TextAlignment="Right" FontSize="50" x:Name="TimeTextBlock" Height="150" Width="1000" HorizontalAlignment="Right" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" Margin="0,0,20,0" />
